// Place your settings in this file to overwrite default and user settings.
{
  "typescript.check.tscVersion": false,
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Material Theme Palenight",
  "editor.fontFamily": "'DM Mono', 'Roboto Mono', Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.lineHeight": 26,
  "editor.fontWeight": "400",
  "workbench.iconTheme": "material-icon-theme",
  "editor.wordWrap": "on",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.next": true,
    "**/.DS_Store": true,
    "node_modules": true,
    "yarn.lock": true
  },
  "files.associations": {
    "*.js": "javascriptreact"
  }
}
